<h1>Listing custom_questions</h1>

<table>
  <tr>
    <th>Question</th>
    <th>Question type</th>
    <th>Sort order</th>
    <th>Permission level</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @custom_questions.each do |custom_question| %>
  <tr>
    <td><%= custom_question.question %></td>
    <td><%= custom_question.question_type_id %></td>
    <td><%= custom_question.sort_order %></td>
    <td><%= custom_question.permission_level %></td>
    <td><%= link_to 'Show', custom_question %></td>
    <td><%= link_to 'Edit', edit_custom_question_path(custom_question) %></td>
    <td><%= link_to 'Destroy', custom_question, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Custom question', new_custom_question_path %>
