<h1>Listing custom_question_answers</h1>

<table>
  <tr>
    <th>Question</th>
    <th>Answer</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @custom_question_answers.each do |custom_question_answer| %>
  <tr>
    <td><%= custom_question_answer.question_id %></td>
    <td><%= custom_question_answer.answer %></td>
    <td><%= link_to 'Show', custom_question_answer %></td>
    <td><%= link_to 'Edit', edit_custom_question_answer_path(custom_question_answer) %></td>
    <td><%= link_to 'Destroy', custom_question_answer, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Custom question answer', new_custom_question_answer_path %>
